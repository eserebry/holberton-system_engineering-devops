#Upstart script that starts a Gunicorn instance to serve web_flask/6-number_odd_or_even.py from your AirBnB_clone_v2
description "Gunicorn application server running airbnb-onepage"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid ubuntu
setgid www-data

script
        cd /home/ubuntu/AirBnB_clone_v2
        exec gunicorn --bind 0.0.0.0:8000 web_flask.6-number_odd_or_even:app
end script
